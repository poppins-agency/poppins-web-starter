{
  "name": "pop-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run ssl-dev & next dev",
    "ssl-dev": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "serve storybook-static",
    "pull-storyblok-components": "eval $(grep '^SB_SPACE_ID' .env) && storyblok pull-components --space $SB_SPACE_ID",
    "generate-sb-types": "eval $(grep '^SB_SPACE_ID' .env) && storyblok-generate-ts source=./components.$SB_SPACE_ID.json target=./src/types/component-types-sb titlePrefix='' titleSuffix='SB' compilerOptions.unknownAny=true compilerOptions.additionalProperties=false  ",
    "gen:update-sb": "npm run pull-storyblok-components && npm run generate-sb-types",
    "analyzer": "ANALYZE=true npm run build",
    "chromatic": "npx chromatic"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@storyblok/react": "^3.0.8",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@vercel/analytics": "^1.1.2",
    "caniuse-lite": "^1.0.30001643",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.1.7",
    "lucide-react": "^0.417.0",
    "next": "^14.2.5",
    "next-sitemap": "^4.2.3",
    "react": "^18.3.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-onboarding": "^8.2.6",
    "@storybook/blocks": "^8.2.6",
    "@storybook/cli": "^8.2.6",
    "@storybook/nextjs": "^8.2.6",
    "@storybook/react": "^8.2.6",
    "@storybook/test": "^8.2.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "chromatic": "^10.7.1",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "storyblok-generate-ts": "^2.0.2",
    "storybook": "^8.2.6",
    "tailwindcss": "^3.3.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5"
  }
}

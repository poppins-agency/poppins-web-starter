{
  "name": "poppins-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap",
    "dev": "npm run ssl-dev & next dev",
    "ssl-dev": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "lint": "gts lint",
    "start": "next start",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "pull-storyblok-components": "eval $(grep '^SB_SPACE_ID' .env) && storyblok pull-components --space $SB_SPACE_ID",
    "generate-sb-types": "eval $(grep '^SB_SPACE_ID' .env) && storyblok-generate-ts source=./components.$SB_SPACE_ID.json target=./src/types/component-types-sb titlePrefix='' titleSuffix='SB' compilerOptions.unknownAny=true compilerOptions.additionalProperties=false  ",
    "gen:update-sb": "npm run pull-storyblok-components && npm run generate-sb-types",
    "analyzer": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.15",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@storyblok/react": "^3.0.15",
    "@storyblok/richtext": "^2.1.0",
    "@studio-freight/lenis": "^1.0.42",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.59.4",
    "@trendyol-js/react-carousel": "^3.0.2",
    "@vercel/analytics": "^1.3.1",
    "@vercel/postgres": "^0.10.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.3.0",
    "framer-motion": "^11.11.6",
    "gsap": "^3.12.5",
    "lucide-react": "^0.451.0",
    "next": "^14.2.15",
    "next-sitemap": "^4.2.3",
    "react": "^18.3.1",
    "react-animated-cursor": "^2.11.2",
    "react-compare-slider": "^3.1.0",
    "react-confetti": "^6.1.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.3.0",
    "react-select": "^5.8.1",
    "sharp": "^0.33.5",
    "storyblok": "^3.34.0",
    "superjson": "2.2.1",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-animation-delay": "^2.0.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/classnames": "^2.3.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^22.7.5",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "^14.2.15",
    "gts": "^5.3.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "storyblok-generate-ts": "^2.1.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "ct3aMetadata": {
    "initVersion": "7.13.1"
  }
}
